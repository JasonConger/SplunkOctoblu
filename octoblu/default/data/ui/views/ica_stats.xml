    <panel>
      <title>Storage Health</title>
      <single>
        <search base="baseRTT">
          <query>stats latest(storage_read_lat) AS read_lat latest(storage_write_lat) AS write_lat |             eval health=if((read_lat&gt;6 OR write_lat&gt;15), 4, if((read_lat&gt;4 OR write_lat&gt;10), 3, if((read_lat&gt;3 OR write_lat&gt;3), 2, 1))) |              rangemap field=health low=1-1 elevated=2-2 high=3-3 severe=4-4</query>
        </search>
        <option name="classField">range</option>
        <option name="field">health</option>
        <option name="additionalClass">icon-only</option>
        <option name="linkView">search</option>
        <option name="drilldown">none</option>
      </single>
      <chart>
        <title></title>
        <search base="baseRTT">
          <query>eval thold=10 | timechart avg(storage_*_lat) avg(thold) | rename avg(storage_read_lat) AS "Read Latency" avg(storage_write_lat) AS "Write Latency" avg(thold) AS "Threshold"</query>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">collapsed</option>
